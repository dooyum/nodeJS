      color:#FFFFFF; z-index:99999; }
  .koemei_widget .koemei_tour_overlay,
  .koemei_widget .koemei_tour_overlay .tour_body { width:100%; height:100%; }
  .koemei_widget .tour_navigation { cursor:pointer; display:table-cell; vertical-align:middle; width:70px; }
  .koemei_widget .tour_section { width:100%; display:table-cell; vertical-align:middle; }
