  .koemei_widget .koemei_video_info .icon.info_author_icon { background-image:url('assets/@1x/icon-info-author.png'); }
  .koemei_widget .koemei_video_info .icon.info_category_icon { background-image:url('assets/@1x/icon-info-cat.png'); }
