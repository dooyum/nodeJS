  .koemei_widget .bar_sections a:hover .icon.binders_button_icon{ background-image:url('assets/@1x/icon-snippets-active.png'); }




  /*
  ==============================================================
  LOGIN
  ==============================================================
  */
  .koemei_widget .kw_login_overlay { position:absolute; width:100%; height:100%; }
  .koemei_widget .kw_login_overlay .wrap { position:absolute; width:100%; z-index: 99999; }

  .koemei_widget .kw_login_overlay a,
  .koemei_widget .kw_login_overlay a:active,
  .koemei_widget .kw_login_overlay a:visited { text-decoration:none; }
  .koemei_widget .kw_login_overlay a:hover { text-decoration:none; }
  .koemei_widget .kw_login_overlay .top { margin:40px 0 30px; }
  .koemei_widget .kw_login_overlay .top .title { font-size:18px; margin-bottom:10px; font-size:18px; color:#FFFFFF; text-align:center; }
  .koemei_widget .kw_login_overlay .top .caption { font-size:14px;  color:#adadad; text-align:center; }

  .koemei_widget .kw_login_overlay .divider { width:230px; margin:30px auto; height:0px; border-bottom:1px solid #474847; }

  .koemei_widget .kw_login_overlay .content { width:230px; margin:30px auto; }

  .koemei_widget .kw_login_overlay .input_wrap { opacity:.85; Alpha(Opacity=85); margin-bottom:10px; }
  .koemei_widget .kw_login_overlay input { width:230px; height:30px; line-height:30px; padding:5px 10px;
      font-size:13px; color:#DDD; border:0; background:#5c5c5c; }
  .koemei_widget .kw_login_overlay .login_error { cursor:pointer; background:#dc3e3e; width:230px; height:70px; margin-bottom:10px; display:table; }
  .koemei_widget .kw_login_overlay .login_error .message { color:white; font-size:14px; height:70px; vertical-align:middle; display:table-cell; text-align:center; }
  .koemei_widget .kw_login_overlay .account_retry { display:inline-block; height:30px; width:100%; line-height:30px;
      font-size: 13px; color:#FFFFFF; background:#179F55; text-align:center; cursor:pointer; }

  .koemei_widget .kw_login_overlay .account_button { float:left; display:inline-block; height:30px; width:110px; line-height:30px;
      font-size: 13px; text-align:center; cursor:pointer; border-width: 0px; font-family:'an-regular', 'Helvetica Neue', Helvetica, Arial, sans-serif; }
  .koemei_widget .kw_login_overlay .account_login { margin-right:10px; background:#179F55; color:#FFFFFF; }
  .koemei_widget .kw_login_overlay .account_signup { float:left; color:#179F55; background:#FFFFFF; }
  .koemei_widget .kw_login_overlay .button_wrap { width:230px; }

  .koemei_widget .kw_login_overlay .account_login:hover,
  .koemei_widget .kw_login_overlay .account_retry:hover { background:#19AF5E; }
  .koemei_widget .kw_login_overlay .account_login:active,
  .koemei_widget .kw_login_overlay .account_retry:active { background:#179F55; }
  .koemei_widget .kw_login_overlay .account_signup:hover { background:#F2F2F2; }
  .koemei_widget .kw_login_overlay .account_signup:active { background:#FFFFFF; }



  /*
  ==============================================================
  GENERAL POPUP
  ==============================================================
  */
.koemei_widget .overlay { position:fixed; top:0; right:0; bottom:0; left:0; cursor:pointer; z-index:10 }
.koemei_widget .popup { padding:15px; background:rgba(0,0,0,0.8); position:absolute; font-size:12px; color:#FFFFFF; z-index:9999; }
  .koemei_widget .popup .heading { color:#AAAAAA; letter-spacing:1px; margin-bottom:10px;
      font-size:10px; font-family: an-regular, 'Helvetica Neue', Helvetica, Arial, sans-serif; }
  .koemei_widget .popup .popup_section { margin-bottom:15px; }
  .koemei_widget .popup .popup_section.last { margin-bottom:0; }
  .koemei_widget .popup .popup_section .content { margin-bottom:0; }
  .koemei_widget .popup .divider { background-color:#494949; height:1px; margin-bottom:15px; }
  .koemei_widget .popup input,
  .koemei_widget .popup select,
  .koemei_widget .popup textarea { width:100%; font-size:12px; color:#555555; background:#FFFFFF; border:0; }
  .koemei_widget .popup input,
  .koemei_widget .popup select,
  .koemei_widget .popup select option { height:25px; line-height:25px; box-sizing:border-box; -moz-box-sizing:border-box; -webkit-box-sizing:border-box; }
  .koemei_widget .popup input { padding:0 8px; line-height:25px; }
  .koemei_widget .popup select,
  .koemei_widget .popup select option { padding:5px 5px 5px 5px; }
  .koemei_widget .popup a { cursor:pointer; }

  /* todo states */
  .koemei_widget .popup .share_button_wrap { margin-bottom:10px; }
  .koemei_widget .popup .share_button { display:inline-block; width:36px; height:36px; float:left; margin-right:10px;
      background-repeat:no-repeat; background-position:center center; }
  .koemei_widget .popup .share_button.last { margin-right:0; }
  .koemei_widget .popup .facebook_share { background-image:url('assets/@1x/share-fb.png'); }
  .koemei_widget .popup .twitter_share { background-image:url('assets/@1x/share-twit.png'); }
  .koemei_widget .popup .google_share { background-image:url('assets/@1x/share-g.png'); }
  .koemei_widget .popup .email_share { background-image:url('assets/@1x/share-em.png'); }
  .koemei_widget .popup .koemei_share { background-image:url('assets/@1x/share-lms.png'); }

  .koemei_widget .popup .share_url_wrap { margin-bottom:15px; }
  .koemei_widget .popup .share_url { padding-left:25px;
      background-image:url('assets/@1x/note-link.png'); background-repeat:no-repeat; background-position:5px center; }

  .koemei_widget .popup .embed_url_wrap { }
  .koemei_widget .popup .embed_url { }

  .koemei_widget .popup .arrow {
  opacity: 0.8;
  filter: Alpha(Opacity=80);
  width: 0;
  height: 0;
  position: absolute;
  border: 8px solid transparent; }

  .koemei_widget .popup .arrow.bottom {
    border-top: 8px solid black;
    border-bottom: none;
    bottom: -8px;
    left: 50%;
    margin-left: -8px; }
  .koemei_widget .popup .arrow.top {
    border-bottom: 8px solid black;
    border-top: none;
    top: -8px;
    left: 50%;
    margin-left: -8px; }
  .koemei_widget .popup .arrow.left {
    border-right: 8px solid black;
    border-left: none;
    left: -8px;
    top: 50%;
    margin-top: -8px; }
  .koemei_widget .popup .arrow.right {
    border-left: 8px solid black;
    border-right: none;
    right: -8px;
    top: 50%;
    margin-top: -8px; }


  /* todo states */
  .koemei_widget .popup .button { width:100%; height:25px; line-height:15px; padding:5px; text-align:center;
      font-size:13px; color:#FFFFFF; background:#AAAAAA; cursor:pointer; border-width: 0px; font-family: an-regular, 'Helvetica Neue', Helvetica, Arial, sans-serif; }
  .koemei_widget .popup .button.positive { background:#66839a; }
  .koemei_widget .popup .button.negative { background:#dc3e3e; }

  .koemei_widget .popup .button:hover { background:#BBBBBB; }
  .koemei_widget .popup .button.positive:hover { background:#19AF5C; }
  .koemei_widget .popup .button.negative:hover { background:#F24444; }


  .koemei_widget .popup ul.link_list li {  }
  .koemei_widget .popup ul a { display:inline-block; padding:5px 8px; width:100%; border-radius:3px; box-sizing:border-box; -moz-box-sizing:border-box; -webkit-box-sizing:border-box; }
  .koemei_widget .popup ul a,
  .koemei_widget .popup ul a:active,
  .koemei_widget .popup ul a:visited { color:white; text-decoration:none; }
  .koemei_widget .popup ul a:hover { text-decoration:none; background:#66839a; }


  /* specific popup styles */
  .koemei_widget .share_video_popup { width:204px; }
  .koemei_widget .share_playlist_popup { width:204px; }
  .koemei_widget .share_snippet_popup { width:214px; }
  .koemei_widget .playlist_popup { width:200px; }
  .koemei_widget .profile_popup { width:200px; }
  .koemei_widget .profile_popup .profile_user { margin-left:40px; height:24px; line-height:24px; }
  .koemei_widget .profile_popup .profile_picture { float:left; margin-left:-40px; width:24px; height:24px; border-radius:12px; background:#444;
      background-repeat:no-repeat; background-position:top left; background-size:contain; }


  /* popup tip */
  .koemei_widget .popup.tip { padding:8px 10px; }







