  .koemei_widget .kw_bar_options a .icon.playlist_button_icon { background-image:url('assets/@1x/icon-add.png'); background-position:4px 3px; }
  .koemei_widget .kw_bar_options a:hover .icon.playlist_button_icon,
  .koemei_widget .kw_bar_options a.active .icon.playlist_button_icon { background-image:url('assets/@1x/icon-add-active.png'); }
  .koemei_widget .kw_bar_options a .icon.share_button_icon { background-image:url('assets/@1x/icon-share.png'); background-position:0px 2px; }
  .koemei_widget .kw_bar_options a:hover .icon.share_button_icon,
  .koemei_widget .kw_bar_options a.active .icon.share_button_icon { background-image:url('assets/@1x/icon-share-active.png'); }
  .koemei_widget .kw_bar_options a .icon.like_button_icon { background-image:url('assets/@1x/icon-like.png'); background-position:0px 0px; }
  .koemei_widget .kw_bar_options a:hover .icon.like_button_icon,
  .koemei_widget .kw_bar_options a.active .icon.like_button_icon { /*kw_icon_like_active*/ }
  .koemei_widget .kw_bar_options a .icon.profile_button_icon { /*kw_icon_profile*/ background-position:3px 4px; }
  .koemei_widget .kw_bar_options a:hover .icon.profile_button_icon,
  .koemei_widget .kw_bar_options a.active .icon.profile_button_icon { background-image:url('assets/@1x/icon-profile-active.png'); }
  .koemei_widget .kw_bar_options .like_button .like_button_count { vertical-align:baseline; font-size:11px; color:white; }
  .koemei_widget .kw_bar_options a.like_button.liked .icon.like_button_icon { /*kw_icon_like_active*/ }

  .koemei_widget .kw_bar_options a .icon.connect_videos_button_icon { width:26px; height:26px;  xbackground-position:4px 3px; }
  .koemei_widget .kw_bar_options a.active .icon.connect_videos_button_icon,.koemei_widget .videos_section .entry .video_connect_option.active { background-image:url('assets/@1x/icon-modal-connect-active.png'); }

  .koemei_widget .bar_sections { float:left; }
  .koemei_widget .bar_sections ul, .bar_sections li { list-style: none outside none; margin:0; }
  .koemei_widget .bar_sections ul { height:32px; }
  .koemei_widget .bar_sections ul li { float:left; display:inline-block; height:32px; margin-right:5px; }

  .koemei_widget .bar_sections a { display:inline-block; width:32px; height:32px; padding:4px; border-radius:3px; box-sizing:border-box; }
  .koemei_widget .bar_sections a,
  .koemei_widget .bar_sections a:active,
  .koemei_widget .bar_sections a:visited { text-decoration:none; }
