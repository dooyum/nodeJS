  .koemei_widget .widget_bar .login_button:hover { background:#F2F2F2; }
  .koemei_widget .widget_bar .login_button:active { background:#FFFFFF; }

  .koemei_widget .kw_bar_options { float:right; }
  .koemei_widget .kw_bar_options ul, .kw_bar_options li { list-style: none outside none; margin:0; }
  .koemei_widget .kw_bar_options ul { height:32px; }
  .koemei_widget .kw_bar_options ul li { float:left; display:inline-block; height:32px; margin-left:5px; }

  .koemei_widget .kw_bar_options a { display:inline-block; height:32px; border-radius:3px; box-sizing:border-box; padding:4px 5px; outline:0; }
  .koemei_widget .kw_bar_options a,
  .koemei_widget .kw_bar_options a:visited { color:white; text-decoration:none; }
  .koemei_widget .kw_bar_options a:hover { color:white; text-decoration:none; opacity:0.8; Alpha(Opacity=80); }
  .koemei_widget .kw_bar_options a.active,
