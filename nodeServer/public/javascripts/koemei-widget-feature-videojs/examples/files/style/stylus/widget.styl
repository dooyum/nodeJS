  .koemei_widget .koemei_video_info .meta a:active { color:#999999; }


  .koemei_widget .video_info_close_button { position:absolute; left:0; top:-1px; width:30px; height:31px; margin:10px; opacity:0.6;  filter: Alpha(Opacity=60);
      cursor:pointer; background-repeat:no-repeat; background-position:center center; }
  .koemei_widget .video_info_close_button:hover { opacity:1;  filter: Alpha(Opacity=100); }

  /*
  ==============================================================
  TOUR
  ==============================================================
  */
  .koemei_widget .video_tour_button { position:absolute; left:31px; top:0; width:26px; height:26px; margin:10px; opacity:0.6;  filter: Alpha(Opacity=60);
      cursor:pointer; background-image:url('assets/@1x/icon-help.png'); background-repeat:no-repeat; background-position:center center; z-index: 999; }
